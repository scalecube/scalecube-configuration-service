couchbase.bucketNamePattern=%s-%s
# roles in enterprise version
# couchbase.bucketRoles=data_reader,data_writer,query_select
couchbase.bucketRoles=bucket_full_access
couchbase.bucketType=COUCHBASE

# quota in megabytes
couchbase.bucketQuota=100
couchbase.bucketReplicas=0
couchbase.bucketIndexReplicas=false
couchbase.bucketEnableFlush=false
